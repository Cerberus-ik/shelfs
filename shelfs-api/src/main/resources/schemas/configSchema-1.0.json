{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "currentGame": {
      "$id": "/properties/currentGame",
      "type": "string",
      "title": "The Currentgame Schema ",
      "default": "",
      "examples": [
        "Shelfs"
      ]
    },
    "onlineStatus": {
      "$id": "/properties/onlineStatus",
      "type": "string",
      "title": "The Onlinestatus Schema ",
      "default": "",
      "examples": [
        "Online"
      ]
    },
    "token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "token",
      "examples": [
        "aopwhjertwopj0w945j0j30ajwer0gp9fjw3049jwß3e4to9kawp.eti0ß3"
      ]
    },
    "pluginDirectory": {
      "$id": "/properties/pluginDirectory",
      "type": "string",
      "title": "The Plugindirectory Schema ",
      "default": "",
      "examples": [
        "plugins"
      ]
    },
    "commandPrefix": {
      "$id": "/properties/commandPrefix",
      "type": "string",
      "title": "The CommandPrefix Schema ",
      "default": "!",
      "examples": [
        "!",
        "#",
        "."
      ]
    },
    "pluginDescription": {
      "$id": "/properties/pluginDescription",
      "type": "string",
      "title": "The PluginDescription Schema ",
      "default": "Your plugin",
      "examples": [
        "Your plugin"
      ]
    },
    "io": {
      "$id": "/properties/pluginDescription",
      "type": "string",
      "title": "The Io Schema ",
      "default": "MySQL",
      "examples": [
        "MySQL"
      ]
    },
    "database": {
      "$id": "/properties/database",
      "type": "object",
      "properties": {
        "ioPassword": {
          "$id": "/properties/database/properties/ioPassword",
          "type": "string",
          "title": "The Iopassword Schema ",
          "default": "",
          "examples": [
            "a safe password"
          ]
        },
        "ioUser": {
          "$id": "/properties/database/properties/ioUser",
          "type": "string",
          "title": "The Iouser Schema ",
          "default": "user",
          "examples": [
            "user"
          ]
        },
        "ioDb": {
          "$id": "/properties/database/properties/ioDb",
          "type": "string",
          "title": "The Iodb Schema ",
          "default": "shelfs",
          "examples": [
            "shelfs",
            "shelfsDB"
          ]
        },
        "ioHost": {
          "$id": "/properties/database/properties/ioHost",
          "type": "string",
          "title": "The Iohost Schema ",
          "default": "host",
          "examples": [
            "123.123.123.123",
            "host.yourHost.com"
          ]
        },
        "ioPort": {
          "$id": "/properties/database/properties/ioPort",
          "type": "string",
          "title": "The Ioport Schema ",
          "default": "3306",
          "examples": [
            "3306"
          ]
        }
      }
    }
  }
}